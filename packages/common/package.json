        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/common": "^5.7.3",
        "@nestjs/core": "^5.7.3",
        "chalk": "^2.4.2",
        "lodash": "^4.17.11",
        "log4js": "^4.0.2",
        "moment": "^2.24.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.4.0",
        "stacktrace-js": "^2.0.0"
    },
    "devDependencies": {
        "@nestjs/testing": "^5.7.3",
        "@types/jest": "^24.0.9",
        "jest": "^23.5.0",
        "ts-jest": "^23.1.3",
        "ts-node-dev": "^1.0.0-pre.32",
        "tsconfig-paths": "^3.8.0",
        "typescript": "^3.3.3333"
    },
    "jest": {
