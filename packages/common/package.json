{
    "name": "@x-liquid/common",
    "version": "1.0.0",
    "main": "dist/index.js",
    "license": "MIT",
    "scripts": {
        "build": "rimraf dist && tsc",
        "test": "jest",
        "test:watch": "jest --watch",
